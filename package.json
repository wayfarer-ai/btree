{
  "name": "@wayfarer-ai/btree",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "tsup-node src/index.ts --watch --format cjs,esm --dts",
    "build": "npm run clean && tsup-node src/index.ts --format cjs,esm --dts",
    "scripting:generate": "bash scripts/generate-script-parser.sh",
    "typecheck": "tsc --noEmit",
    "test": "CI=true vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@temporalio/workflow": "^1.10.0",
    "@temporalio/activity": "^1.10.0",
    "antlr4": "^4.13.2"
  },
  "devDependencies": {
    "@temporalio/client": "^1.10.0",
    "@temporalio/worker": "^1.10.0",
    "@temporalio/testing": "^1.10.0",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "latest",
    "vitest": "^3.2.4"
  }
}
